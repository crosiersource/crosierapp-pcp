{% if fichaTecnica.id %}

    <div id="fichaTecnica_imagem mt-4">
        <input type="hidden" id="fichaTecnica_imagem_id" name="fichaTecnica_imagem[id]">
        <div class="form-group row">
            <label class="col-form-label col-sm-2"
                   for="fichaTecnica_imagem_imageFile">Imagens</label>
            <div class="col-sm-10">
                <div class="custom-file">
                    <input type="file" multiple id="fichaTecnica_imagem_imageFile"
                           name="fichaTecnica_imagem[imageFile][]"
                           class="custom-file-input"
                           data-url="{{ url('prod_fichaTecnica_formImagemFileUpload', {fichaTecnica: fichaTecnica.id}) }}">
                    <label for="fichaTecnica_imagem_imageFile"
                           class="custom-file-label"></label>
                </div>
            </div>
        </div>
    </div>


    <div class="row" id="filesUl">
        {% include 'fichaTecnica_imagens_filesUl.html.twig' %}
    </div>


{% endif %}